					<attribute id="VisualTemplate" type="FixedString" value="be71f516-2ea6-8037-6302-4c10294dce32" /> -- REQUIRED - The actual model UUID -- REQUIRES interact bones
					<attribute id="DisplayName" type="TranslatedString" handle="hdcc11ffdgcf24g4888gaf6bg9b5f4fc57385" version="1" /> -- REQUIRED - Mouseover tooltip name of the gameobject
					<attribute id="Type" type="FixedString" value="item" />
