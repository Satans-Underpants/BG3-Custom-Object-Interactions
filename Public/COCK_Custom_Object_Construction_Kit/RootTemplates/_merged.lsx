					<attribute id="CanBeMoved" type="bool" value="True" /> -- REQUIRED - Not needed for objects using the template
					<attribute id="WalkThrough" type="bool" value="True" /> -- OPTIONAL
				</node>
					<children>
						<node id="OnUsePeaceActions">
							<children>
								<node id="Action">
									<attribute id="ActionType" type="int32" value="14" /> -- REQUIRED - "14" = Sit - "12" Use/interact action
									<children>
										<node id="Attributes">
											<attribute id="Animation" type="FixedString" value="" />
										</node>
									</children>
								</node>
							</children>
						</node>
					</children>
			<!-- Example Object -->

				<node id="GameObjects">
					<attribute id="Name" type="FixedString" value="COCK_Test" /> -- REQUIRED - For worldspace object "Name_UUID" output
